<AnchorPane prefHeight="438.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.gjkt.view.DruhyZnamekController">
    <children>
        <HBox layoutX="-1.0" layoutY="-1.0" prefHeight="39.0" prefWidth="600.0">
