                <Label prefHeight="38.0" prefWidth="184.0" text="Druhy známek">
                    <font>
                        <Font size="26.0" />
                    </font>
                    <HBox.margin>
                        <Insets bottom="2.0" left="20.0" top="2.0" />
                    </HBox.margin>
                </Label>
                <HBox prefHeight="42.0" prefWidth="226.0">
                    <children>
