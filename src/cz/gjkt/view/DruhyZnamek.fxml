                                <Insets bottom="5.0" left="2.0" right="2.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleDomuButton" text="Domů">
                            <HBox.margin>
                                <Insets bottom="5.0" left="2.0" right="2.0" top="2.0" />
                            </HBox.margin></Button>
                    </children>
                    <HBox.margin>
                        <Insets left="200.0" />
                    </HBox.margin>
                    <padding>
                        <Insets top="10.0" />
                    </padding>
                </HBox>
            </children>
        </HBox>
        <TableView fx:id="tableView" layoutY="38.0" prefHeight="400.0" prefWidth="600.0" />
